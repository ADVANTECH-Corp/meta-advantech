+++ b/arch/arm64/boot/dts/qcom/advantech-pmic-pins.dtsi
@@ -0,0 +1,34 @@
+#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
+#include <dt-bindings/pinctrl/qcom,pmic-mpp.h>
+
+&pm8916_gpios {
+
+	usb_hub_reset_pm: usb_hub_reset_pm {
+		pinconf {
